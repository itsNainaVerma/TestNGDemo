<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="TestNG Demo">
  <test name="include and exclude groups with regular expression">
  <groups>
  <run>
    <exclude name = "smoke"/>
    <include name = "defect.*"/>
  </run>
  </groups>
    <classes>
      <class name="dependencyTesting.DemoWebsiteTest"/>
    </classes>
  </test> <!-- include and exclude groups with regular expression -->
</suite> <!-- TestNG Demo -->
